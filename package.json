{
	"name": "sidebar-translate",
	"displayName": "sidebar-translate",
	"description": "A extension what let you study english",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sidebar-translate.helloWorld",
		"onView:sbts"
	],
	"main": "./extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "translate",
					"title": "Sidebar Translate",
					"icon": "icon/sidebar.svg"
				}
			]
		},
		"views": {
			"translate": [
				{
					"id": "sbts",
					"name": "translate",
					"contextualTitle": "Sidebar translate"
				}
			]
		},
		"commands": [
			{
				"command": "sbts.helloWorld",
				"title": "Hello World",
				"icon":"icon/search.svg"

			}
		],
		"menu" : {
			"view/title":{
				"command":"sbts.helloworld",
				"when":"view == sbts",
				"group": "navigation"
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}