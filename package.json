{
	"name": "untitle",
	"displayName": "untitle1",
	"description": "untitle1",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:untitle.helloWorld",
		"onView:treeView-item",
		"onCommand:itemClick"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "untitle.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"viewsContainers": {                        
        "activitybar": [                       
            {
                "id": "treeView",               
                "title": "treeView",            
                "icon": "icon/dot-fill.svg"    
            }
        ]
    },
    "views": {                                  
        "treeView": [                           
            {
                "id": "treeView-item",          
				"name": "item",                 
				"icon":"icon/dep.svg",
				"contextualTitle": "Sidebar Translate"                 
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.45.0",
		"typescript": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0"
	},
	"dependencies": {
		"ftp": "^0.3.10",
		"jsonc-parser": "^0.4.2",
		"minimist": "^0.2.1",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"vscode": "^1.1.37"
	}
}
